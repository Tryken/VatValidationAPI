{
  "supportedCountryCodes": {
    "AT": {
      "validationRegex": "^ATU[0-9]{8}$",
      "adapter": "EU"
    },
    "BE": {
      "validationRegex": "^BE[01][0-9]{9}$",
      "adapter": "EU"
    },
    "BG": {
      "validationRegex": "^BG[0-9]{9,10}$",
      "adapter": "EU"
    },
    "CH": {
      "validationRegex": "^CHE-[0-9]{3}\\.[0-9]{3}\\.[0-9]{3}$",
      "adapter": "CH"
    },
    "CY": {
      "validationRegex": "^CY[0-9A-Z]{8}[A-Z]{1}$",
      "adapter": "EU"
    },
    "CZ": {
      "validationRegex": "^CZ[0-9]{8,10}$",
      "adapter": "EU"
    },
    "DE": {
      "validationRegex": "^DE[0-9]{9}$",
      "adapter": "EU"
    },
    "DK": {
      "validationRegex": "^DK[0-9]{8}$",
      "adapter": "EU"
    },
    "EE": {
      "validationRegex": "^EE[0-9]{9}$",
      "adapter": "EU"
    },
    "EL": {
      "validationRegex": "^(EL|GR)[0-9]{9}$",
      "adapter": "EU"
    },
    "ES": {
      "validationRegex": "^ES([0-9]{8}[A-Z])|([A-Z][0-9]{8})|([A-Z][0-9]{7}[A-Z])$",
      "adapter": "EU"
    },
    "FI": {
      "validationRegex": "^FI[0-9]{8}$",
      "adapter": "EU"
    },
    "FR": {
      "validationRegex": "^FR[0-9A-Z]{2}[0-9]{9}$",
      "adapter": "EU"
    },
    "GB": {
      "validationRegex": "^GB([0-9]{9}|[0-9]{12}|GD[0-9]{3}|HA[0-9]{3})$",
      "adapter": "EU"
    },
    "HR": {
      "validationRegex": "^HR[0-9]{11}$",
      "adapter": "EU"
    },
    "HU": {
      "validationRegex": "^HU[0-9]{8}$",
      "adapter": "EU"
    },
    "IE": {
      "validationRegex": "^IE[0-9]((([0-9]|[A-Z]|\\+\\*)[0-9]{5}[A-Z])|([0-9]{6}[A-Z]{2}))$",
      "adapter": "EU"
    },
    "IT": {
      "validationRegex": "^IT[0-9]{11}$",
      "adapter": "EU"
    },
    "LT": {
      "validationRegex": "^LT([0-9]{9}|[0-9]{12})$",
      "adapter": "EU"
    },
    "LU": {
      "validationRegex": "^LU[0-9]{8}$",
      "adapter": "EU"
    },
    "LV": {
      "validationRegex": "^LV[0-9]{11}$",
      "adapter": "EU"
    },
    "MT": {
      "validationRegex": "^MT[0-9]{8}$",
      "adapter": "EU"
    },
    "NL": {
      "validationRegex": "^NL[0-9]{9}B[0-9]{2}$",
      "adapter": "EU"
    },
    "PL": {
      "validationRegex": "^PL[0-9]{10}$",
      "adapter": "EU"
    },
    "PT": {
      "validationRegex": "^PT[0-9]{9}$",
      "adapter": "EU"
    },
    "RO": {
      "validationRegex": "^(RO)?[0-9]{2,10}$",
      "adapter": "EU"
    },
    "SE": {
      "validationRegex": "^SE[0-9]{12}$",
      "adapter": "EU"
    },
    "SI": {
      "validationRegex": "^SI[0-9]{8}$",
      "adapter": "EU"
    },
    "SK": {
      "validationRegex": "^SK[0-9]{10}$",
      "adapter": "EU"
    }
  },
  "adapter": {
    "eu": {
      "baseUrl": "https://ec.europa.eu/taxation_customs/vies/rest-api",
      "splitRegex": "^([A-Z]{2}-?)(.+)$"
    },
    "ch": {
      "wsdlUrl": "https://www.uid-wse-a.admin.ch/V5.0/PublicServices.svc"
    }
  }
}
